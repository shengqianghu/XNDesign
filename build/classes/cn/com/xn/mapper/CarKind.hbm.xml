<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="cn.com.xn.model">
	<class name="CarKind" table="carkind">
		<id name="id" column="car_kind_id" type="integer" >
			<generator class="native"></generator>
		</id>
		<property name="number" column="number" type="integer" ></property>
		<property name="brand" column="brand" type="string" length="20" ></property>
		<property name="country" column="country" type="string" length="50" ></property>
	</class>
</hibernate-mapping>
