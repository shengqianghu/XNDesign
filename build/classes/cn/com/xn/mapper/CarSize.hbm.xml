<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="cn.com.xn.model">
	<class name="CarSize" table="carsize">
		<id name="id" column="t_car_size_id" type="integer" >
			<generator class="native"></generator>
		</id>
		<many-to-one name="car" class="Car" column="car_number"  ></many-to-one>
		<many-to-one name="size" class="Size" column="size_number"  ></many-to-one>
	</class>
</hibernate-mapping>
