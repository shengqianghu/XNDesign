<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="cn.com.xn.model">
	<class name="Size" table="size">
		<id name="id" column="t_size_id" type="integer" >
			<generator class="native"></generator>
		</id>
		<property name="number" column="size_number" type="integer" ></property>
		<property name="size" column="size" type="string" length="20" ></property>
	</class>
</hibernate-mapping>
