<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="cn.com.xn.model">
	<class name="CarColor" table="carcolor">
		<id name="id" column="t_car_color_id" type="integer" >
			<generator class="native"></generator>
		</id>
		<many-to-one name="car" class="Car" column="car_number"  ></many-to-one>
		<many-to-one name="color" class="Color" column="color_number"  ></many-to-one>
	</class>
</hibernate-mapping>
