<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="cn.com.xn.model">
	<class name="Customer" table="customer">
		<id name="id" column="custo_id" type="integer" >
			<generator class="native"></generator>
		</id>
		<property name="name" column="name" type="string" length="20" ></property>
		<property name="userName" column="user_name" type="string" length="50" ></property>
		<property name="password" column="user_password" type="string" length="50" ></property>
		<property name="time" column="time" type="timestamp"  ></property>
		<property name="sex" column="sex" type="string" length="10" ></property>
		<property name="phone" column="telephone" type="string" length="11" ></property>
	</class>
</hibernate-mapping>
